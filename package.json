{
  "name": "iss-spotter",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "cd build && http-server",
    "all": "git add -A",
    "staging": "git push origin mvp-staging",
    "mkdir": "mkdir -p build",
    "build": "npm run clean && npm run mkdir && npm run build:html && npm run build:css && npm run build:js && cp src/images/earth.png build/",
    "watch": "npm run watch:html & npm run watch:css & npm run watch:js",
    "clean": "rm -rf build",
    "build:html": "npm run clean:html && cp index.html build/",
    "watch:html": "npm run build:html && chokidar index.html -c 'npm run build:html'",
    "clean:html": "rm -f build/index.html",
    "build:css": "cp css/normalize.css build/ && npm run clean:css && lessc --source-map css/index.less build/$npm_package_name.$npm_package_version.css",
    "watch:css": "npm run build:css && chokidar 'css/**/*.less' -c 'npm run build:css'",
    "clean:css": "rm -f build/$npm_package_name.$npm_package_version.css build/$npm_package_name.$npm_package_version.css.map",
    "build:js": "npm run clean:js && webpack",
    "watch:js": "npm run build:js && webpack --watch",
    "clean:js": "rm -f build/$npm_package_name.$npm_package_version.js build/$npm_package_name.$npm_package_version.js.map",
    "test": "mocha --compilers js:babel-register test/**/*.js*",
    "push-staging": "git push origin mvp-staging"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "less": "^2.7.1",
    "mocha": "^3.0.2",
    "react-addons-test-utils": "^15.3.1",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "magellan-coords": "^1.1.0",
    "material-ui": "^0.15.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.1",
    "react-gmaps": "^1.5.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-tap-event-plugin": "^1.0.0",
    "react-tooltip": "^3.1.8",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  }
}
